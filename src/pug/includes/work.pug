include ../utils/mixins-work

section#about.about
  .about__wrapper.bg-info
    .container-fluid 
      .about__body.row.no-gutters.bg-primary.flex-column
        .about__button 
          +button-grey("Know more")
        .about__sliding-container.d-none
          .about__title.bg-danger.Text-Style-6 About us
        +sticky("А","bout us")  
        .about__plaza.about-plaza.row.no-gutters
          .about-plaza__body.col-md-7
            .about-plaza__column.about-plaza__column_high.row.no-gutters
              .about-plaza__item
                img(src="assets/img/work/clutch.png",alt="img")
              .about-plaza__item
                img(src="assets/img/work/polygon.png",alt="img")
            .w-100
            .about-plaza__column.row.no-gutters
              .about-plaza__item
                img(src="assets/img/work/polygon.png",alt="img")
              .about-plaza__item
                img(src="assets/img/work/upwork.png",alt="img")
              .about-plaza__item.col-md-3
                img(src="assets/img/work/polygon.png",alt="img")
          .about__discription.col-md-5
            p.Text-Style-9 Lorem ipsum dolor sit amet, consectetur adipiscing elit. Maecenas   varius tortor nibh, sit amet tempor nibh finibus et. Aenean eu enim   justo. Vestibulum aliquam hendrerit molestie.
